            color: #6DB8A5;
        }

        .form-container {
            max-width: 400px;
            margin: auto;
        }

        .form-container h2 {
            font-size: 24px;
            color: #6E6E6E;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .form-container label {
            display: block;
            margin-top: 15px;
            font-weight: bold;
            color: #6E6E6E;
        }

        .form-container input {
            width: 100%;
            padding: 10px;
            border: none;
            border-bottom: 2px solid #333;
            outline: none;
            margin-top: 5px;
        }

        .form-container button {
            margin-top: 25px;
            padding: 12px 25px;
            border: none;
            background-color: #EADAD6;
            color: #000;
            font-size: 16px;
            border-radius: 25px;
            cursor: pointer;
            box-shadow: 2px 2px 5px #aaa;
        }

        .form-container button:hover {
            background-color: #d1c2be;
        }
    </style>
